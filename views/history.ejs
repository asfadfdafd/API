<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>API Request History</title>
</head>
<body>
    <h1>API Request History</h1>
    <ul>
        <% history.forEach(entry => { %>
            <li>
                <strong>API:</strong> <%= entry.userRequest.api %> <br>
                <strong>Input:</strong> <%= JSON.stringify(entry.userRequest.input) %> <br>
                <strong>Response:</strong> <%= JSON.stringify(entry.response) %> <br>
                <strong>Timestamp:</strong> <%= entry.createdAt.toLocaleString() %>
                <hr>
            </li>
        <% }) %>
    </ul>
    <%- include('partials/footer') %>
</body>
</html>
